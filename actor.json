{
  "actorSpecification": 1,
  "name": "apollo-data-scraper",
  "title": "Apollo.io Data Scraper",
  "description": "Scrape contact data from Apollo.io lists. Enter a URL and number of pages to extract contact information including names, emails, phone numbers, and more.",
  "version": "1.0.0",
  "meta": {
    "templateId": "js-playwright"
  },
  "input": "./INPUT_SCHEMA.json",
  "dockerfile": "./Dockerfile",
  "readme": "./README.md",
  "storages": {
    "dataset": {
      "actorSpecification": 1,
      "title": "Apollo Contacts",
      "views": {
        "overview": {
          "title": "Overview",
          "transformation": {
            "fields": [
              "firstName",
              "lastName",
              "fullName",
              "email",
              "phone",
              "title",
              "company"
            ]
          },
          "display": {
            "component": "table",
            "properties": {
              "firstName": {
                "label": "First Name",
                "format": "text"
              },
              "lastName": {
                "label": "Last Name",
                "format": "text"
              },
              "fullName": {
                "label": "Full Name",
                "format": "text"
              },
              "email": {
                "label": "Email",
                "format": "text"
              },
              "phone": {
                "label": "Phone",
                "format": "text"
              },
              "title": {
                "label": "Title",
                "format": "text"
              },
              "company": {
                "label": "Company",
                "format": "text"
              }
            }
          }
        }
      }
    }
  }
}



